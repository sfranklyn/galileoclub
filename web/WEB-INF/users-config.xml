<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="1.2"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <managed-bean>
        <managed-bean-name>users</managed-bean-name>
        <managed-bean-class>galileoclub.jsf.beans.UsersBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>app</property-name>
            <value>#{app}</value>
        </managed-property>        
        <managed-property>
            <property-name>visit</property-name>
            <value>#{visit}</value>
        </managed-property>
        <managed-property>
            <property-name>pnrCountsBean</property-name>
            <value>#{pnrCounts}</value>
        </managed-property>
    </managed-bean>
</faces-config>
